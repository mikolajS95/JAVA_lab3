public class Student
{
    String imie;
    String nazwisko;
    int index;
    boolean czyMaStypendium;

    Student(String imie, String nazwisko, int index, boolean czyMaStypendium)
    {
     this.imie=imie;
     this.nazwisko=nazwisko;
     this.index=index;
     this.czyMaStypendium=czyMaStypendium;
    }

}
